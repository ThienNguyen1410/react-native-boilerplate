{
  "presets": ["module:metro-react-native-babel-preset"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./app"],
        "alias": {
          "@app" : "./app" ,
          "@core": "./app/core",
          "@data": "./app/data",
          "@domain" : "./app/domain",
          "@presetation" : "./app/presentation",
          "@util": "./app/utils"
        }
      }
    ]
  ]
}
